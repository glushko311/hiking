{% extends "::base.html.twig" %}
{% block body %}
<div class="page_title" id="page_title">
    <h1>{{ blog.title }} </h1>
</div>
    <div class="field field_created">
        <h3>{{ blog.created|date("d-m-Y H:m") }}</h3>
    </div>
    <article>
        <div class="container blog-container">
            <div class="field field-body">{{ blog.body|raw }}</div>
        </div>
    </article>
{% endblock %}

